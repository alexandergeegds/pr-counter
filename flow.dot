digraph G {
    node [shape=Mrecord];

    start [label="",shape=circle,color=black,width=0.2];
    end [label="",shape=circle,color=black,style=filled,width=0.2];
    help [label="{Show help page | Prompt to continue}"];
    check [label="Check for previous data",shape=diamond,margin=0];
    summary [label="{Show the summary page | Ask for confirmation the data is correct}"];

    using_api [label="",shape=point];
    send [label="Send API request"];
    send_succeed [label="Displays number returned by API"];
    send_failed [label="Displays (human-readable) error message"];
    generate [label="Generate URL"];
    generate_succeed [label="Displays GitHub URL"];

    subgraph cluster_data {
        label="Gather input data";
        labeljust=l;
        color=black;

        q_use_api [label="Ask whether the API or GitHub search should be used",shape=octagon,margin=0];

        g_token [label="Prompt for (optional) API token",shape=rectangle];
        g_repos [label="Prompt for (optional) repository list",shape=rectangle];
        g_authors [label="Prompt for (optional) author list",shape=rectangle];
        g_dates [label="Prompt for (optional) start and end dates",shape=rectangle];

        q_use_api -> g_token [label="use API"];
        g_token -> g_repos;
        q_use_api -> g_repos [label="use search"];
        g_repos -> g_authors;
        g_authors -> g_dates;
    }

    start -> help;
    help -> check;
    check -> summary [label="previous data exists"];
    check -> q_use_api [label="no previous data found"];

    g_dates -> summary;
    summary -> q_use_api [label="rejected"];
    summary -> using_api [label="confirmed"];
    using_api -> send [label="if using GitHub API"];
    using_api -> generate [label="if using GitHub search"];
    send -> send_succeed [label="request succeeds"];
    send -> send_failed [label="request fails"];
    generate -> generate_succeed;
    send_succeed -> end;
    send_failed -> end;
    generate_succeed -> end;
}


